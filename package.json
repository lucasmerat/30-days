    "mongoose": "^5.5.6",
    "morgan": "~1.9.0",
    "nodemon": "^1.19.0",
    "passport-instagram": "^1.0.0",
    "request": "^2.88.0",
    "util": "^0.12.0"
  },
  "description": "",
  "main": "server.js",
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
